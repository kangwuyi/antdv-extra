{
  "name": "antdv-extra",
  "version": "0.0.2",
  "type": "module",
  "description": "A collection of loading indicators animated with CSS for VueJS 3",
  "author": "Chanwit Piromplad <kingkong2103@gmail.com>",
  "scripts": {
    "fix-tsc-paths": "tsc-alias -p ./tsconfig.bundle.json",
    "tsc-check": "vue-tsc --noEmit",
    "tsc-generate": "vue-tsc --project ./tsconfig.bundle.json",
    "build-lib": "vite build --config=vite.config.bundle.ts",
    "build": "node --max_old_space_size=4096 ./node_modules/vite/bin/vite.js build",
    "bundle": "npm run tsc-check && npm run build-lib",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "main": "bundle/index.umd.cjs",
  "module": "bundle/index.es.js",
  "types": "bundle/index.d.ts",
  "files": [
    "bundle",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kangwuyi/antdv-extra.git"
  },
  "keywords": [
    "vue3",
    "antdv-extra"
  ],
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/randomcolor": "^0.5.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "@vue/eslint-config-standard-with-typescript": "^9.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.0.0",
    "ant-design-vue": "4.2.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-storybook": "^9.0.18",
    "eslint-plugin-vue": "^10.4.0",
    "jest": "^30.0.5",
    "jiti": "^2.4.2",
    "playwright": "^1.51.1",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prismjs": "^1.30.0",
    "randomcolor": "^0.6.2",
    "tailwindcss": "^4.1.11",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "unplugin-vue-define-options": "^3.0.0-beta.19",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^3.0.4",
    "vue3-toastify": "^0.2.8",
    "yaml": "^2.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
